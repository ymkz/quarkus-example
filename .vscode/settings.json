{
  "java.configuration.updateBuildConfiguration": "automatic",
  "[java]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[markdown][html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[yaml][dockercompose]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  }
}
